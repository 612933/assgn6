import java.util.*;
public class acad {

	public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
     
		System.out.print(a+b);

	}

}

3.
import java.util.*;
public class acad {
	public void  sum(int c,int d)
    {
		System.out.print(c+d);
    }

	public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
     acad m=new acad();
        m.sum(a,b);
	}

}
